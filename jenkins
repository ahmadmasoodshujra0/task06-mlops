pipeline {
    agent any

    stages {
        stage('Checkout') {
            steps {
                // Check out your code from your GitHub repository
                checkout([$class: 'GitSCM', branches: [[name: '*/main']], userRemoteConfigs: [[url: 'https://github.com/faizanaslam786/Mlops_task6.git']]])
            }
        }
        
       
        stage('Run Tests') {
            steps {
                // Run your test cases using pytest
                bat 'python test.py'
            }
        }
    }
}
